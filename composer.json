{
    "name": "laravelir/ticketable",
    "description": "a ticketable package for laravel",
    "homepage": "https://github.com/laravelir/ticketable",
    "type": "library",
    "version": "0.1.0",
    "keywords": [
        "laravel",
        "laravel-package",
        "laravel ticket",
        "ticketable",
        "ticket"
    ],
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com",
            "role": "maintainer",
            "homepage": "https://github.com/miladimos"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravelir\\Ticketable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelir\\Ticketable\\Providers\\TicketableServiceProvider"
            ],
            "aliases": {
                "Ticketable": "Laravelir\\Ticketable\\Facades\\Ticketable"
            }
        }
    },
    "require": {
        "php": ">=7.4|^8.0"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
